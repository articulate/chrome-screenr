{
  "name": "Screenr",
  "version": "1.0",
  "description": "Screenr Chrome extensions",
  "browser_action": {
    "default_icon": "screenr.ico"
  },
   "permissions":  [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts":[{
	"matches": ["http://*/*", "https://*/*"],
	"js":["jquery.min.js","jquery.ba-throttle-debounce.js","content.js"]
  }]
}
